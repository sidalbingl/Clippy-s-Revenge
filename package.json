{
  "name": "clippys-revenge",
  "version": "1.0.0",
  "description": "Evil Clippy haunts your code",
  "main": "dist/main/main.js",
  "scripts": {
    "dev": "npm run build:main && concurrently \"npm run watch:main\" \"npm run dev:renderer\" \"npm run start:electron\"",
    "build:main": "tsc -p tsconfig.main.json",
    "watch:main": "tsc -p tsconfig.main.json --watch",
    "dev:renderer": "vite",
    "start:electron": "wait-on dist/main/main.js && electron .",
    "build": "tsc -p tsconfig.main.json && vite build",
    "start": "electron .",
    "generate:sounds": "node scripts/generateSounds.js",
    "test:response": "ts-node src/mcp/responseEngine/responseEngine.test.ts",
    "example:response": "ts-node src/mcp/responseEngine/example.ts",
    "clean": "rimraf dist node_modules",
    "reinstall": "npm run clean && npm install"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "electron": "^28.0.0",
    "postcss": "^8.4.32",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.3.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.8",
    "wait-on": "^7.2.0"
  },
  "dependencies": {
    "@google/genai": "^1.30.0",
    "@google/generative-ai": "^0.24.1",
    "chokidar": "^3.5.3",
    "dotenv": "^17.2.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
